package:graphql
class:UsersSpec
scenario:Creating new user

request:
"""
mutation createUser {
  createUser(input: {username: <username>, password: <password>, firstName: <firstName>, lastName: <lastName>, address: <address>, phone: <phone>, email: <email>, money: <money>}) {
    id
    username
    firstName
    lastName
    address
    phone
    email
    money
  }
}
"""


response:
"""
{
  "data": {
    "createUser":
      {
        "id": <id>,
        "username": <username>,
        "firstName": <firstName>,
        "lastName": <lastName>,
        "address": <address>,
        "phone": <phone>,
        "email": <email>,
        "money": <money>
      }
  }
}
"""


